{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "claude-built",
  "title": "Claude Built",
  "description": "Quick access to all Claude-built skills, tools, and scripts",
  "icon": "robot",
  "author": "loudog",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Claude Built Palette",
      "description": "Search and execute Claude-built items",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.83.2",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "setup": "mkdir -p ~/.claude/bin && cp scripts/generate-registry.sh ~/.claude/bin/generate-claude-built-registry && chmod +x ~/.claude/bin/generate-claude-built-registry && ~/.claude/bin/generate-claude-built-registry",
    "generate": "~/.claude/bin/generate-claude-built-registry"
  }
}
